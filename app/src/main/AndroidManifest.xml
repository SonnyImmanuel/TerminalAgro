<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.terminalagro">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".TambahItem"></activity>
        <activity android:name=".SyaratKetentuan" />
        <activity android:name=".DaftarToko" />
        <activity android:name=".EditProfil" />
        <activity android:name=".Login" />
        <activity android:name=".ProfilSaya" />
        <activity android:name=".Splashscreen" />
        <activity android:name=".AlamatBaru">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>